{
  "name": "DSA Tracker",
  "version": "1.0",
  "description": "Track your DSA progress.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "DSA Tracker"
  },
  "icons": {
    "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
